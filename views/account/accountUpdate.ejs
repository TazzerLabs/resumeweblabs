<%- include("../includes/header.ejs", {title: "Update " + account.Fname + ' ' + account.Lname}); %>

<h1>Update <%= account.Fname%></h1>
<form action="/account/update" method="get">
    <input type="hidden" id="account_id" name="account_id" value="<%= account.account_id %>">
    First Name: <input id="Fname" name="Fname" type="text" value="<%= account.Fname%>"/>
    <br />
    <br />
    Last Name: <input id="Lname" name="Lname" type="text" value="<%= account.Lname%>"/>
    <br />
    <br />
    Email: <input id="email" name="email" type="text" value="<%= account.email%>"/>
    <br />
    <br />

    <input type="submit" />

</form>

<a href="/account/all"><< back</a>

<%- include("../includes/footer.ejs"); %>

</html>